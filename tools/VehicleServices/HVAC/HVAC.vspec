#
# Vehicle HVAC Service Group Specification file
#

#
# The root branch for highlevel vehicle services.
#
HVAC:
  type: branch
  description: Root node for HVAC services.

HVAC.Service1:
  type: procedure
  description: Handles Service1

HVAC.Service1.Input:
  type: iostruct
  description: Struct defining the input parameters

HVAC.Service1.Input.Param1:
  type: property
  datatype: string
  description: The input parameter

HVAC.Service1.Version:
  type: attribute
  datatype: string
  default: 0.0.1
  description: HVAC Service1 version

#include HVAC-sub.vspec HVAC
